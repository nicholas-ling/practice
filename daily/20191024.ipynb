   "source": [
    "def dfs(s, k, subset, ret):\n",
    "    if k<0:\n",
    "        return\n",
    "    if k==0:\n",
    "        ret.append(subset)\n",
    "        return\n",
    "    for i in range(len(s)):\n",
    "        dfs(s[i+1:], k-s[i], [s[i]]+subset, ret)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 21,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "[]\n"
     ]
    }
   ],
   "source": [
    "ret = []\n",
    "dfs([] , 100, [], ret)\n",
    "print(ret)"
   ]
