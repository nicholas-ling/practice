   "source": [
    "def max_profit(arr):\n",
    "    min_p, max_p = max(arr), 0 \n",
    "    for value in arr:\n",
    "        if value < min_p:\n",
    "            min_p = value\n",
    "        else:\n",
    "            max_p = max(max_p, value-min_p)\n",
    "    return max_p"
   ]
  },
