   "source": [
    "def power(x, y):\n",
    "    if y==0: \n",
    "        return 1\n",
    "    if y==1:\n",
    "        return x\n",
    "    if y%2 == 0:\n",
    "        sqroot = power(x, y//2)\n",
    "        return sqroot*sqroot\n",
    "    else:\n",
    "        return x * power(x, y-1)"
   ]
