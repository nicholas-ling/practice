   "source": [
    "def max_sum(arr):\n",
    "    s, m = 0, 0\n",
    "    for num in arr:\n",
    "        s += num\n",
    "        m = max(m, s)\n",
    "        if s < 0:\n",
    "            s = 0\n",
    "    return m\n",
    "\n",
    "max_sum([-5,-1,-8,-9])\n",
    "max_sum([1,6,-9,100,-101,300, -209])"
   ]
