   "source": [
    "def dfs(ret, arr, current, start):\n",
    "    for i in range(start, len(arr)):\n",
    "        c = current[:]\n",
    "        c.append(arr[i])\n",
    "        ret.append(c)\n",
    "        dfs(ret, arr, c, i+1)"
   ]
  },
  {
